routine rec_fib;

procedures:

	proc rec_fib(n: int): int {
		locals:
			var a: int;
			var b: int;
		instructions:
			if (n == 0) {
				return 0;
			}
			if (n == 1) {
				return 1;
			}
			else {
				a = rec_fib(n - 1);
				b = rec_fib(n - 2);
				return a + b;
			}
	}

begin: [
    instructions:
        write rec_fib(7);
]
