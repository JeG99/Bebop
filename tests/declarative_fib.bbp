routine dec_fib;

procedures:

    proc fib (num: int): int {
	    locals:
            var n: int;
			var m: int;
			var o: int;

		instructions:
			n = 1;
			m = 0;
			repeat (num > 0) {
				o = n;
				n = n + m;
				m = o;
				num = num - 1;
			}
	    return m;
    }

begin: [
	locals:
		var param: int;

    instructions:
		param = 2 + 5;

        write fib(param);
]
