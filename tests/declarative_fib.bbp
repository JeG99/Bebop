routine dec_fib;

procedures:
    proc fib (num: int): int {
		    locals:
				var n: int;
				var m: int;
				var o: int;
		
		    instructions:
				n = 1;
				m = 0;

				repeat (num > 0) {
					o = num;
					n = n + m;
					m = o;
					num = num - 1;
				}

		    return m; 
    }

begin: [
    instructions:
        write fib(4);
]
